                                        <input type="text" placeholder="Ubicación">
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="aa-single-advance-search">
                                        <select>
                                            <option selected>Tipo Inmueble</option>
                                            <option>Casa </option>
                                            <option>Departamento </option>
                                            <option>En Desarrollo</option>
                                            <option>Local</option>
                                            <option>Oficina</option>
                                            <option>Suite</option>
                                            <option>Villa</option>
                                            <option>Terreno</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="aa-single-advance-search">
                                        <select>
                                            <option value="0" selected>Opción</option>
                                            <option value="1">Venta</option>
                                            <option value="2">Arriendo</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="aa-single-advance-search">
                                        <select>
                                            <option value="0" selected>Habitaciones</option>
                                            <option value="1">1</option>
                                            <option value="2">2</option>
                                            <option value="3">3</option>
                                            <option value="4">4</option>
                                            <option value="4">+5</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <div class="aa-single-advance-search">
                                        <input class="aa-search-btn" type="submit"
                                            value="Buscar">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="aa-advance-search-bottom">
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="aa-single-filter-search">
                                        <span>AREA (m2)</span>
                                        <span>DESDE</span>
                                        <span id="skip-value-lower" class="example-val">30.00</span>
                                        <span>HASTA</span>
                                        <span id="skip-value-upper" class="example-val">100.00</span>
                                        <div id="aa-sqrfeet-range" class="noUi-target
                                            noUi-ltr noUi-horizontal noUi-background">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="aa-single-filter-search">
                                        <span>PRECIO ($)</span>
                                        <span>DESDE</span>
                                        <span id="skip-value-lower2"
